# Environment variables for React App
# Copy this file to .env.local and update with your values

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5000
REACT_APP_API_TIMEOUT=10000

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_TRACKING=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=false

# App Configuration
REACT_APP_NAME=AgriConnect
REACT_APP_VERSION=1.0.0

# Payment Configuration (if needed)
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key_here

# Analytics (if needed)
REACT_APP_GA_TRACKING_ID=your_google_analytics_id_here
